<%- include("partials/header") -%>
	

    <section class="featured-offers  parallax " >
  

		
			<div class="searchrow">
			<%myData.forEach(element=>{%>
				<form action="/read"  method="post">
				<button type="submit " style="all:unset; cursor: pointer;">
					
					<button class="searchfeature">
						
						<div class="featured-offer-front">
							<div class="featured-offer-photo">
								<img src='uploads\<%=element.image[0]%>'  alt="" />
								<div class="type-container">
									<div class="estate-type"><%=element.submitpropertytype%></div>
									<div class="transaction-type"><%=element.submittransaction%></div>
								</div>
								
							</div>
							<div class="featured-offer-text">
								<h4 class="featured-offer-title"><%=element.landmark%>, <%=element.submitregion%>, <%=element.submitcity%>, <%=element.submitstate%>, INDIA</h4>
								<p><%=element.submitmessage.slice(0,300)%></p>
							</div>
							<div class="featured-offer-params">
								<div class="featured-area">
									<img src="images/area-icon.png" alt="" /><%=element.submitarea%>m<sup>2</sup>
								</div>
								<div class="featured-rooms">
									<img src="images/rooms-icon.png" alt="" /><%=element.submitbedroom%>
								</div>
								<div class="featured-baths">
									<img src="images/bathrooms-icon.png" alt="" /><%=element.submitbathroom%>
								</div>							
							</div>
							<div class="featured-price">
								â‚¹<%=element.submitprice%>
							</div>
							
						</div>
						
						
							<input name="_id" type="hidden" class="input-full main-input" value="<%=element._id%>" />
					<input name="email" type="hidden" class="input-full main-input" value="<%=element.email%>" />
						<!-- <div class="featured-offer-back">
							
							
							<div class="button">	
								
									<span>read more</span>
									<div class="button-triangle"></div>
									<div class="button-triangle2"></div>
									<div class="button-icon"><i class="fa fa-search"></i></div>
								
							</div>
						</div> -->
				
					</form>
						

		</div>

	</button>
	<br>
		  
<%});%>
			</div>
		

		

</section>

				
<!-- Move to top button -->

<div class="move-top">
	<div class="big-triangle-second-color"></div>
	<div class="big-icon-second-color"><i class="jfont fa-lg">&#xe803;</i></div>
</div>	


